{
  "name": "butler-ci-cli",
  "version": "3.0.0",
  "description": "CLI para interactuar con Jenkins Pipelines",
  "main": "dist/index.js",
  "bin": {
    "butler-ci-cli": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/usarral/butler-ci-cli.git"
  },
  "bugs": {
    "url": "https://github.com/usarral/butler-ci-cli/issues"
  },
  "homepage": "https://github.com/usarral/butler-ci-cli#readme",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "lint": "eslint . --ext .ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run test:run && npm run build"
  },
  "keywords": [
    "jenkins",
    "cli",
    "pipeline",
    "devops",
    "automation"
  ],
  "author": "usarral",
  "license": "MIT",
  "packageManager": "pnpm@10.15.0",
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.6.2",
    "@vitest/coverage-v8": "^4.0.5",
    "@vitest/ui": "^4.0.5",
    "axios-mock-adapter": "^2.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.5"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "inquirer": "^12.9.6",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2"
  }
}
